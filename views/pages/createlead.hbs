<form action="createlead" method="post">
    <input type="text" name="fname" value="{{lead.fname}}">
    <input type="text" name="lname" value="{{lead.lname}}">
    <input type="email" name="email" value="{{lead.email}}">
    <input type="tel" name="contact" value="{{lead.contact}}">
    <input type="text" name="website" value="{{lead.website}}">
    <button type="submit">Create Lead</button>
</form>